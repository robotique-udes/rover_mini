<launch>
	<param name="/baud_rate" type="int" value="115200" />
	<param name="/qpps" type="string" value="0,0,0,0" />
	<param name="/accel" type="string" value="0,0,0,0" />
	<param name="battery_low" type="int" value="11" />
	<param name="battery_high" type="int" value="18" />
	<param name="mech_dist" type="string" value="7.254,10.5,10.5,10.073" />

	<node name="motor_velocity_controller" pkg="osr" type="motor_velocity_controller.py" respawn="true" output="screen"/>

	<node name="twist_to_motors" pkg="differential_drive" type="twist_to_motors.py" >
		<param name="base_width" value="0.425" />
		<remap from="twist" to="cmd_vel" />
	</node>

	<node name="left_pid_velocity" pkg="differential_drive" type="pid_velocity.py" output="screen">
		<remap from="motor_cmd" to="lmotor_cmd" />
		<remap from="wheel_vel" to="lwheel_vel" />
		<remap from="wheel" to="lwheel" />
		<remap from="wheel_vtarget" to="lwheel_vtarget" />
		<param name="out_min" value="-127" />
		<param name="out_max" value="127" />
                <param name="Kp" value="1" />
                <param name="Ki" value="100" />
                <param name="Kd" value="0" />
		<param name="ticks_meter" value="26069" />
	</node>

	<node name="right_pid_velocity" pkg="differential_drive" type="pid_velocity.py" output="screen" >
		<remap from="motor_cmd" to="rmotor_cmd" />
		<remap from="wheel_vel" to="rwheel_vel" />
		<remap from="wheel" to="rwheel" />
		<remap from="wheel_vtarget" to="rwheel_vtarget" />
		<param name="out_min" value="-127" />
		<param name="out_max" value="127" />
                <param name="Kp" value="1" />
                <param name="Ki" value="100" />
                <param name="Kd" value="0" />
		<param name="ticks_meter" value="26069" />
	</node>

	<node name="diff_tf" pkg="differential_drive" type="diff_tf.py" >
		<remap from="odom" to="wheel_odom" />
		<param name="base_width" value="0.425" />
                <param name="encoder_min" value="-2147483648" />
                <param name="encoder_max" value="2147483,647" />
		<param name="ticks_meter" value="26069" />
	</node>
</launch>
	
