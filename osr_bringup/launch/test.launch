<launch>
	<node name="twist_to_motors" pkg="differential_drive" type="twist_to_motors" >
		<param name="base_width" value="0.425" />
		<remap from="twist" to="cmd_vel" />
	</node>

	<node name="left_pid_velocity" pkg="differential_drive" type="pid_velocity" >
		<remap from="motor_cmd" to="lmotor_cmd" />
		<remap from="wheel_vel" to="lwheel_vel" />
		<remap from="wheel" to="lwheel" />
		<remap from="wheel_vtarget" to="lwheel_vtarget" />
		<param name="out_min" value="-127" />
		<param name="out_max" value="127" />
	</node>

	<node name="right_pid_velocity" pkg="differential_drive" type="pid_velocity" >
		<remap from="motor_cmd" to="rmotor_cmd" />
		<remap from="wheel_vel" to="rwheel_vel" />
		<remap from="wheel" to="rwheel" />
		<remap from="wheel_vtarget" to="rwheel_vtarget" />
		<param name="out_min" value="-127" />
		<param name="out_max" value="127" />
	</node>

	<node name="diff_tf" pkg="differential_drive" type="diff_tf" >
		<remap from="odom" to="wheel_odom" />
		<param name="base_width" value="0.425" />
	</node>
</launch>
	
